{
  "name": "zzz-seelie-sync",
  "private": true,
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:nightly": "cross-env RELEASE_NIGHTLY=true vite build",
    "build:release": "cross-env RELEASE=true vite build",
    "preview": "tsc && vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:prod": "cross-env NODE_ENV=production eslint src --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc -b --noEmit",
    "version": "node -p \"require('./package.json').version\"",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major"
  },
  "dependencies": {
    "@trim21/gm-fetch": "^0.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^22.17.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "vite-plugin-monkey": "^6.0.1"
  }
}
